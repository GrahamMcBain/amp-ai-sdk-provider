{
  "name": "amp-ai-sdk-provider",
  "version": "0.1.0",
  "description": "Coding agent provider for Vercel AI SDK that exposes Amp's development tools, project context, and agent capabilities",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint src/**/*.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:unit": "vitest run tests/unit",
    "test:compliance": "vitest run tests/compliance",
    "type-check": "tsc --noEmit",
    "ci": "npm run lint && npm run type-check && npm run build && npm run test",
    "prepublishOnly": "npm run ci"
  },
  "keywords": [
    "ai",
    "amp",
    "coding-agent",
    "developer-tools",
    "vercel",
    "ai-sdk",
    "provider",
    "sourcegraph",
    "code-generation",
    "project-context",
    "toolbox"
  ],
  "author": "Graham McBain <graham@ampcode.com>",
  "license": "MIT",
  "dependencies": {
    "@sourcegraph/amp-sdk": "0.1.0-3e67faf1b0e66cf0d3bf2985979efbcac0554a79"
  },
  "peerDependencies": {
    "@ai-sdk/provider": "^1.0.0",
    "@ai-sdk/provider-utils": "^2.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/GrahamMcBain/amp-ai-sdk-provider.git"
  },
  "homepage": "https://github.com/GrahamMcBain/amp-ai-sdk-provider",
  "bugs": {
    "url": "https://github.com/GrahamMcBain/amp-ai-sdk-provider/issues"
  }
}
